  "description": "Simple component",
  "main": "index.js",
  "scripts": {
    "watch": "webpack --watch",
    "clean": "rimraf dist",
    "pack": "npm run pack:dev",
    "pack:dev": "npm run clean && cross-env NODE_ENV=development BABEL_ENV=nohmr webpack --verbose --display-modules --display-reasons --display-chunks --display-error-details",
    "pack:prod": "npm run clean && cross-env NODE_ENV=production webpack --verbose --display-modules --display-reasons --display-chunks --display-error-details",
    "start": "npm run start:dev",
    "start:dev": "npm run clean && node devServer.js",
    "start:prod": "npm run clean && npm run pack:prod && cross-env NODE_ENV=production node server.js",
    "start:nohmr": "npm run clean && cross-env BABEL_ENV=nohmr node devServer.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
